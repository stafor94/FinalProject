<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.stafor.gachonclass">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher_gachon"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round_gachon"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity"
            android:configChanges="orientation|screenSize">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".SplashAcitivty" android:theme="@style/NoTitle" />
        <activity android:name=".ImageActivity" android:theme="@style/NoTitle" />
        <activity android:name=".SeekActivity"></activity>
    </application>

</manifest>